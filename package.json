{
  "name": "realchart",
  "version": "0.9.16",
  "description": "Wooritech charting library",
  "main": "index.js",
  "author": "",
  "license": "license.txt",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsc",
    "bundle": "tsc && rollup --config rollup.config.js",
    "lib": "rm -rf dist/.npm/dist && tsc -p tsconfig.lib.json && rollup --config rollup.config.js --environment BUILD:lib && node version-check.js",
    "libdebug": "rm -rf dist/.npmdebug/dist && tsc -p tsconfig.lib.json && rollup --config rollup.config.js --environment BUILD:libdebug && node version-check.js",
    "bundle:e2e": "tsc && rollup --config rollup.config.js --environment BUILD:e2e",
    "dbundle": "tsc && rollup --config rollup.config.js --environment BUILD:debug",
    "dbundle2": "tsc && rollup --config rollup.config.js --environment BUILD:debug && yarn test",
    "dbw": "rollup --config rollup.config.js --environment BUILD:debugw --watch",
    "pbundle": "tsc && rollup --config rollup.config.js --environment BUILD:prod",
    "pub": "tsc && rollup --config rollup.config.js --environment BUILD:pub",
    "pdeploy": "npm run pbundle && node compress.js",
    "test": "npm run test:model && npm run test:pw",
    "test:chrome": "yarn dbundle && npm run test:model && npx playwright test --project=chromium",
    "test:model": "mocha --exit --timeout 10000 -r ts-node/register -r test/unit/dom-mock.js 'test/{,!(e2e)/**/}/*.spec.ts' 'test/unit/**/*Test*.ts'",
    "test:pw": "yarn dbundle && npx playwright test --trace on",
    "tsdoc": "npx typedoc --entryPointStrategy expand ./src/model --entryPointStrategy expand ./src/common --json ./docs/.tdout/model.json",
    "jsdoc": "rm -rf api/jsdoc && npx jsdoc -c jsdoc.json",
    "dldoc": "npm run tsdoc && node dldoc && npx typedoc && node docs/typedoc",
    "dldoc:dev": "node dldoc",
    "typedoc:dev": "node docs/typedoc",
    "dev:www": "npm run dldoc && npx live-server web/realchart",
    "dev": "node app",
    "fiddle": "node exportDemo",
    "docs:demo": "node docsDemo"
  },
  "devDependencies": {
    "@babel/generator": "^7.22.15",
    "@babel/parser": "^7.22.15",
    "@babel/traverse": "^7.22.15",
    "@playwright/test": "^1.38.0",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.1.0",
    "@rollup/pluginutils": "^5.0.2",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "better-docs": "^2.7.2",
    "chai": "^4.3.7",
    "chokidar": "^3.5.3",
    "compressing": "^1.8.0",
    "cors": "^2.8.5",
    "esbuild": "^0.17.19",
    "express": "^4.18.2",
    "jsdoc": "^4.0.2",
    "jsdom": "^21.1.0",
    "jsdom-global": "^3.0.2",
    "live-server": "^1.2.2",
    "marked": "^4.2.12",
    "mocha": "^10.2.0",
    "path": "^0.12.7",
    "prismjs": "^1.29.0",
    "reload": "^3.2.1",
    "replace-in-file": "^6.3.5",
    "rollup": "^3.18.0",
    "rollup-plugin-banner2": "^1.2.2",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^5.2.0",
    "rollup-plugin-modify": "^3.0.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "source-map-resolve": "^0.6.0",
    "taffydb": "^2.7.3",
    "terser": "^5.16.5",
    "ts-loader": "^9.4.2",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typedoc": "^0.25.3",
    "typescript": "^4.9.5",
    "vite": "^4.1.4"
  },
  "dependencies": {
    "bezier-easing": "^2.1.0",
    "jsbarcode": "^3.11.5",
    "puppeteer": "^21.1.0",
    "qrcode": "^1.5.1",
    "html2canvas": "^1.3.2"
  }
}
